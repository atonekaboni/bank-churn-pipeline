   "source": [
    "from sqlalchemy import create_engine\n",
    "from sqlalchemy.engine import URL\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "bdf45ebb-fdef-4cbf-9b00-eb94b54f4f72",
   "metadata": {},
   "outputs": [],
   "source": [
    "target_db_name = \"pgbank\" \n",
    "target_host = \"localhost\" \n",
    "target_port = 5432"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "6b859092-67a0-4d01-93fa-56aa6a95a731",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "User Pass checked.\n"
     ]
    }
   ],
   "source": [
    "pg_user = os.getenv('PG_USER')\n",
    "pg_password = os.getenv('PG_PASSWORD')\n",
    "\n",
    "if pg_user and pg_password:\n",
    "    print(\"User Pass checked.\")\n",
    "else:\n",
    "    print(\"Check CMD\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "94181667-571f-4c97-952e-74b5bef3306a",
   "metadata": {},
   "outputs": [],
   "source": [
    "connection_url = URL.create(\n",
    "            \"postgresql+psycopg2\",\n",
    "            username=pg_user,\n",
    "            password=pg_password,\n",
    "            host=\"localhost\",\n",
    "            database=\"pgbank\",\n",
    "            port=5432\n",
    "        )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "2246cb16-0b6f-41f7-9b42-5effe13951f8",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "connected\n"
     ]
    }
   ],
   "source": [
    "engine = create_engine(connection_url)\n",
    "with engine.connect():\n",
    "            print(\"connected\")"
   ]
  },
